div.foot-navigation {
	clear : both;
	height : 150px;
	font : 9pt Verdana, Geneva, Arial, Helvetica, sans-serif; 
	color : white;
	background : #990099;
	text-align : right; 
	width : 100%; 
}

a.foot-navigation:visited,a.foot-navigation:link {
	color : white;
	background : #990099;
	font : 9pt Verdana, Geneva, Arial, Helvetica, sans-serif;
	text-decoration : none;
	text-align : center;
}

a.foot-navigation:hover {
	color : blue;
	background : #990099;
	font : 9pt Verdana, Geneva, Arial, Helvetica, sans-serif;
	text-decoration : none;
	text-align : center;
}

div.top-navigation {
	position : absolute;
	top: 0px;
	padding-top : 10px;
	height : 32px;
	font : bold 10pt Verdana, Geneva, Arial, Helvetica, sans-serif; 
	color : #DAA520;
	background : #990099;
	text-align : center; 
	width : 100%; 
	z-index: 1;
}

a.top-navigation:visited,a.top-navigation:link {
	color : #DAA520;
	background : #990099;
	font : bold 10pt Verdana, Geneva, Arial, Helvetica, sans-serif;
	text-decoration : none;
	text-align : center;
}

a.top-navigation:hover {
	color : white;
	background : #990099;
	font : bold 10pt Verdana, Geneva, Arial, Helvetica, sans-serif;
	text-decoration : none;
	text-align : center;
}

div.left-navigation {
	position: absolute;
	float : left;
	left: 0px;
	top: 150px;
	width: 200px;
	height: 500px;
}

table.left-navigation {
	text-align : right;
}

td.left-navigation {
	border : 5pt;
	border-style: solid;
	border-color : black;
	text-align : right;
	height : 50px;
	width : 200px;
}

a.drawings {
	background-image: url("/resources/images/navigation/drawings.gif");
	background-repeat: no-repeat;
	z-index: 2;
}
a.drawings:hover {
	background-image: url("/resources/images/navigation/drawings_i.gif");
	background-repeat: no-repeat;
	z-index: 2;
}

